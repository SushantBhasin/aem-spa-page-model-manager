  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "test": "npm run build && karma start --browsers=ChromeHeadless --single-run=true",
    "test-debug": "npm run build && karma start --browsers=Chrome_with_debugging",
    "build": "npm run linter && babel src --out-dir dist",
    "docs": "jsdoc -c ./jsdoc_conf.json && doxdox \"src/**/*.+(js|jsx)\" -i \"src/Helpers.js,src/ModelRouter.js\" --layout templates/DOCUMENTATION.hbs --output DOCUMENTATION.md",
    "preversion": "npm run test && npm run build && npm run docs && npm run readme",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "readme": "node_modules/markdown-include/bin/cli.js markdown-include.config.json",
    "prepublishOnly": "npm version && npm run docs && npm run readme",
    "postpublish": "git commit -am 'publish latest version' && git push origin && git push origin --tags"
  },
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "publishConfig": {
