{
  "name": "@adobe/cq-spa-page-model-manager",
  "version": "1.0.7",
  "auto-changelog": {
    "template": "templates/CHANGELOG.hbs"
  },
  "bugs": {
    "url": "https://www.adobe.com/go/aem6_4_support_en"
  },
  "engines": {
    "npm": ">=5.5.1"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://www.adobe.com/go/aem6_4_docs_spa_en",
  "repository": {},
  "keywords": [
    "the official",
    "adobe",
    "aem",
    "spa editor",
    "experience manager",
    "marketing cloud",
    "cq",
    "reactjs",
    "pagemodelmanager"
  ],
  "main": "dist/cq-spa-page-model-manager.js",
  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "test": "npm run build && cross-env NODE_ENV=test karma start --browsers=ChromeHeadless --single-run=true",
    "test:debug": "cross-env NODE_ENV=test npm run build && cross-env NODE_ENV=test karma start --browsers=Chrome_with_debugging",
    "build": "npm run linter && webpack",
    "build:production": "npm run linter && webpack --mode=production",
    "docs": "jsdoc -c ./jsdoc_conf.json && doxdox \"src/**/*.+(js|jsx)\" -i \"src/Helpers.js,src/ModelRouter.js\" --layout templates/DOCUMENTATION.hbs --output DOCUMENTATION.md",
    "readme": "node node_modules/markdown-include/bin/cli.js markdown-include.config.json",
    "docs-readme": "npm run docs && npm run readme",
    "preversion": "npm run test && npm run build:production && npm run docs-readme",
    "version": "npm run docs && npm run readme && git add -A",
    "postversion": "git push && git push --tags"
  },
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "clone": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "doxdox": "^3.0.0",
    "eslint": "^6.7.2",
    "fetch-mock": "^8.0.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.6.3",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "markdown-include": "^0.4.3",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
