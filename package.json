{
  "name": "@cq/spa-page-model-manager",
  "version": "0.0.4",
  "description": "Registry providing mechanism for managing the model representation of a page",
  "main": "index.js",
  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "test": "npm run build && karma start",
    "test-debug": "npm run build && karma start --browsers=Chrome --single-run=false --auto-watch",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "docs": "jsdoc ./src/",
    "prepublish": "npm-auto-version npm run build",
    "postpublish": "git commit -am 'publish latest version' && git push origin && git push origin --tags"
  },
  "babel": {
    "plugins": [
      [
        "istanbul",
        {
          "exclude": "./tests/*.*"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:CQ/spa-page-model-manager.git"
  },
  "author": "Adobe Systems Inc.",
  "publishConfig": {
    "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-cq-spa-release-local"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "clone": "^2.1.1",
    "custom-event-polyfill": "^0.3.0",
    "webpack": "^1.13.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module"
    }
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^3.5.0",
    "eslint": "4.3.0",
    "eslint-plugin-babel": "4.1.2",
    "inject-loader": "2.0.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.3.3",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.2.0",
    "npm-auto-version": "^1.0.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0"
  }
}