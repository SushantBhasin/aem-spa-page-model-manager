    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "globals": {
    "CustomEvent": true,
    "console": true,
    "document": true,
    "fetch": true,
    "history": true,
    "Promise": true,
    "window": true,
    "XMLHttpRequest": true
  },
  "rules": {
    "no-console": "off"
  },
  "extends": [
    "eslint:recommended"
  ]
}